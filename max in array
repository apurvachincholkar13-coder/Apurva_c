#include<stdio.h>
int main(){
    int arr[2][3];
    int i,j;
printf("Enter the elements: ");
for(i=0;  i<2 ; i++){
    for(int j=0 ; j<3 ; j++){
        scanf("%d" ,&arr[i][j]);
    }
}
int max=arr[0][0];
for(i=0 ; i<2 ; i++){
    for(j=0 ; j<3 ; j++){
        if(max<arr[i][j]){
            max=arr[i][j];
        }
    }
}
printf("maximum is %d " ,max);
return 0;
}
