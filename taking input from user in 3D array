#include<stdio.h>
int main()
{
int a[2][2][3];
printf("Enter elements of array: ");
int k,i,j;
for(k=0;k<2;k++)
{
  for(i=0;i<2;i++)
  {
     for(j=0;j<3;j++)
     {
       scanf("%d",&a[k][i][j]);
     }
  }
}
for(k=0;k<2;k++)
{
  for(i=0;i<2;i++)
  {
     for(j=0;j<3;j++)
     {
       printf(" %d",a[k][i][j]);
     }
     printf("\t");
  }
  printf("\n");
}
return 0; 
}
